<?xml version="1.0" encoding="utf-8"?>
<run>
    <age_contact_matrix_file>contact_matrix_flanders_subpop.xml</age_contact_matrix_file>
    <behaviour_policy>NoBehaviour</behaviour_policy>
    <belief_policy>
        <name>NoBelief</name>
    </belief_policy>
    <contact_log_level>Transmissions</contact_log_level>
    <contact_output_file>true</contact_output_file>
    <disease_config_file>disease_measles.xml</disease_config_file>
    <global_information_policy>NoGlobalInformation</global_information_policy>
    <holidays_file>holidays_none.json</holidays_file>
    <immunity_profile>None</immunity_profile>
    <immunity_rate>0.8</immunity_rate>
    <local_information_policy>NoLocalInformation</local_information_policy>
    <num_days>50</num_days>
    <num_participants_survey>10</num_participants_survey>
    <num_threads>1</num_threads>
    <output_adopted>false</output_adopted>
    <output_cases>true</output_cases>
    <output_persons>false</output_persons>
    <output_prefix></output_prefix>
    <output_summary>false</output_summary>
    <population_file>pop_double_flanders.csv</population_file>
    <region_file>region_double_flanders.csv</region_file>
    <poolsys_file>poolsys_double_flanders.csv</poolsys_file>
    <rng_seed>1</rng_seed>
    <r0>11</r0>
    <seeding_age_max>99</seeding_age_max>
    <seeding_age_min>1</seeding_age_min>
    <seeding_rate>0.002</seeding_rate>
    <start_date>2017-01-01</start_date>
    <stride_log_level>info</stride_log_level>
    <track_index_case>false</track_index_case>
    <use_install_dirs>true</use_install_dirs>
    <vaccine_link_probability>0</vaccine_link_probability>
    <vaccine_profile>Random</vaccine_profile>
    <vaccine_rate>0.8</vaccine_rate>
    <pop_config>
        <write_geogrid>true</write_geogrid>
        <write_population>true</write_population>
        <rng>
            <engine>mrg2</engine>
            <seed>25646</seed>
        </rng>
        <region>
            <region_name>Flanders1</region_name>
            <fragment_centers>true</fragment_centers>
	        <!-- The amount of persons to generate -->
	        <population_size>100000</population_size>
	        <!-- The household reference set to sample from -->
	        <household_profile>households_flanders.xml</household_profile>
	        <geoprofile>
		        <cities>data/flanders_cities2.csv</cities>
		        <commuters>data/flanders_commuting.csv</commuters>
	        </geoprofile>
            <school_size>500</school_size>
            <school_cp_size>20</school_cp_size>
            <university_size>3000</university_size>
            <university_cp_size>20</university_cp_size>
            <workplace_size>20</workplace_size>
	        <university>
		        <!-- Among the 18-26 year olds, a fraction is a student -->
		        <student_fraction>0.50</student_fraction>
		        <!-- Among the students, a fraction commutes -->
		        <commute_fraction>0.50</commute_fraction>
	        </university>
	        <work>
		        <!-- Among the employable population (excluding students), a fraction is active -->
		        <work_fraction>0.70</work_fraction>
		        <!-- Among the active population, a fraction commutes -->
		        <commute_fraction>0.50</commute_fraction>
	        </work>
            <tourist_fraction>0.01</tourist_fraction>
            <work_travel_fraction>0.05</work_travel_fraction>
        </region>
        <region>
            <region_name>Flanders2</region_name>
            <fragment_centers>true</fragment_centers>
            <!-- The amount of persons to generate -->
            <population_size>140000</population_size>
            <!-- The household reference set to sample from -->
            <household_profile>households_flanders.xml</household_profile>
            <geoprofile>
                <cities>data/flanders_cities2.csv</cities>
                <commuters>data/flanders_commuting.csv</commuters>
            </geoprofile>
            <school_size>500</school_size>
            <school_cp_size>20</school_cp_size>
            <university_size>3000</university_size>
            <university_cp_size>20</university_cp_size>
            <workplace_size>20</workplace_size>
            <university>
                <!-- Among the 18-26 year olds, a fraction is a student -->
                <student_fraction>0.50</student_fraction>
                <!-- Among the students, a fraction commutes -->
                <commute_fraction>0.50</commute_fraction>
            </university>
            <work>
                <!-- Among the employable population (excluding students), a fraction is active -->
                <work_fraction>0.70</work_fraction>
                <!-- Among the active population, a fraction commutes -->
                <commute_fraction>0.50</commute_fraction>
            </work>
            <tourist_fraction>0.01</tourist_fraction>
            <work_travel_fraction>0.05</work_travel_fraction>
        </region>
    </pop_config>
</run>
